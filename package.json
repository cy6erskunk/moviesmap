  "license": "ISC",
  "dependencies": {
    "@google/maps": "1.1.3",
    "@googlemaps/react-wrapper": "1.1.42",
    "clone": "2.1.2",
    "core-js": "3.38.1",
    "express": "4.21.1",
    "node-fetch": "2.7.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "8.1.3",
    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "whatwg-fetch": "3.6.20"
  },
  "devDependencies": {
    "@babel/core": "7.25.8",
    "@babel/preset-env": "7.25.8",
    "@babel/preset-react": "7.25.7",
    "@babel/preset-typescript": "7.25.7",
    "@biomejs/biome": "1.9.3",
    "@playwright/test": "^1.48.0",
    "@rspack/cli": "^1.0.9",
    "@rspack/core": "^1.0.8",
    "@types/googlemaps": "3.43.3",
