{
  "name": "moviesmap",
  "version": "1.0.0",
  "description": "SF movie locations on the map",
  "main": "index.js",
  "scripts": {
    "test": "jest test/",
    "test:watch": "jest --watchAll test/",
    "build:dev": "rspack --mode development",
