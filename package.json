    "babel-loader": "9.2.1",
    "deep-freeze": "0.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-loader": "9.5.1",
    "typescript": "5.6.3",
    "uuid": "10.0.0",
    "vercel": "37.8.0"
  },
  "engines": {
    "node": ">=14"
